Color;\r\n  }\r\n}\r\n\r\n.price-card__buttons{\r\n  @include flex(center, flex-start);\r\n}\r\n\r\n.price-card__title{\r\n  font-weight: 800;\r\n  font-size: 20px;\r\n  line-height: 150%;\r\n  letter-spacing: 0.02em;\r\n  color: $fontColor;\r\n  margin-bottom:30px;\r\n}\r\n\r\n.price-card__text{\r\n  font-style: normal;\r\n  font-weight: 500;\r\n  font-size: 18px;\r\n  letter-spacing: 0.03em;\r\n  color: $fontColor;\r\n  margin-bottom:5px;\r\n}\r\n\r\n.price-card__price{\r\n  font-weight: bold;\r\n  font-size: 18px;\r\n  line-height: 150%;\r\n  letter-spacing: 0.02em;\r\n  color: $red;\r\n  margin-top:25px;\r\n  margin-bottom:40px;\r\n}\r\n\r\n.price-card__number{\r\n  position:absolute;\r\n  font-weight: 500;\r\n  font-size: 35px;\r\n  line-height: 120%;\r\n  letter-spacing: 0.03em;\r\n  color: #E7E2E2;\r\n  top:0;\r\n  left:60px;\r\n  top:50%;\r\n  transform:translateY(-50%)\r\n}\r\n\r\n.price-card__right-img{\r\n  position:absolute;\r\n  z-index:0;\r\n  right:30px;\r\n  bottom:0;\r\n}\r\n\r\n.price-card__right-content{\r\n  position:relative;\r\n  z-index:1;\r\n}\r\n\r\n.price-bottom{\r\n  margin-top:40px;\r\n  padding:50px 130px 50px 0;\r\n  background: #FFFFFF;\r\n  box-shadow: 2px 7px 50px rgba(0, 0, 0, 0.05);\r\n  border-radius: 2px;\r\n  @include flex(center, space-between);\r\n}\r\n\r\n.price-bottom__title{\r\n  font-weight: 500;\r\n  font-size: 20px;\r\n  line-height: 120%;\r\n  letter-spacing: 0.03em;\r\n  color: $fontColor;\r\n  padding-left:50px;\r\n  border-left:4px solid $fontColor;\r\n}\r\n\r\n.price-bottom__points{\r\n  width:320px;\r\n}\r\n\r\n.price-bottm__call{\r\n  @include flex(center, center, column);\r\n}\r\n\r\n.price-bottom__call-text{\r\n  margin-top:13px;\r\n  font-weight: 500;\r\n  font-size: 12px;\r\n  line-height: 120%;\r\n  letter-spacing: 0.03em;\r\n  color: $fontColor;\r\n  text-align:center;\r\n}\r\n\r\n.price-content__points{\r\n  position:absolute;\r\n  top:150px;\r\n  right: calc(100% + 20px);\r\n}\r\n","scss\\variables.scss":"$backgroundColor: #F0F0F0;\r\n$fontColor: #30475E;\r\n$secondaryFontColor: #4C4D52;\r\n$red: #F05454;\r\n","scss\\mixins.scss":"@mixin flex($a: center, $b: center, $c: row, $d: no-wrap){\r\n  display:flex;\r\n  align-items:$a;\r\n  justify-content:$b;\r\n  flex-direction:$c;\r\n  flex-wrap: $d;\r\n}\r\n"},"lineCount":null}},"error":null,"hash":"7ed80d24d0879976705007b0699a8dfd","cacheData":{"env":{}}}