n  width:40%;\r\n  height:100%;\r\n}\r\n\r\n.portfolio-slide__right{\r\n  width:55%;\r\n}\r\n\r\n.portfolio-slide__left img{\r\n  width:100%;\r\n  height:100%;\r\n  object-fit:cover;\r\n}\r\n\r\n.portfolit-slide__title{\r\n  font-weight: 800;\r\n  font-size: 20px;\r\n  line-height: 150%;\r\n  letter-spacing: 0.02em;\r\n  color: $fontColor;\r\n  margin-bottom:5px;\r\n}\r\n\r\n.portfolit-slide__subtitle{\r\n  font-size: 16px;\r\n  letter-spacing: 0.03em;\r\n  color: #848484;\r\n}\r\n\r\n.portfolio-slide__main{\r\n  padding:30px 0 40px 0;\r\n  border-top: 1px solid #EBEBEB;\r\n  border-bottom: 1px solid #EBEBEB;\r\n  margin:25px 0;\r\n}\r\n\r\n.portfolit-slide__row, .portfolit-slide__price-row{\r\n  margin-bottom:8px;\r\n  @include flex(center, space-between);\r\n}\r\n\r\n.portfolit-slide__row span:first-child, .portfolit-slide__price-row span:first-child{\r\n  font-weight: 500;\r\n  font-size: 16px;\r\n  letter-spacing: 0.03em;\r\n  color: #939393;\r\n}\r\n\r\n.portfolit-slide__row span:last-child{\r\n  font-weight: 500;\r\n  font-size: 16px;\r\n  line-height: 180%;\r\n  letter-spacing: 0.03em;\r\n  color: $fontColor;\r\n}\r\n\r\n.portfolio-price-span{\r\n  font-weight: bold;\r\n  font-size: 18px;\r\n  letter-spacing: 0.02em;\r\n  color: #F05454;\r\n}\r\n\r\n.portcolio-slide__buttons{\r\n  margin-top:28px;\r\n  @include flex(center)\r\n}\r\n\r\n.portfolio-slide__button-gray{\r\n  @include flex(center, center);\r\n  width:35%;\r\n  height:56px;\r\n  background: $fontColor;\r\n  box-shadow: 5px 2px 14px rgba(0, 0, 0, 0.15);\r\n  border-radius: 2px;\r\n  cursor:pointer;\r\n\r\n  span{\r\n    font-weight: bold;\r\n    font-size: 16px;\r\n    line-height: 150%;\r\n    color: #FFFFFF;\r\n  }\r\n}\r\n\r\n.portfolio-slide__button-light{\r\n  @include flex(center, center);\r\n  width:65%;\r\n  height:56px;\r\n  background: #fff;\r\n  box-shadow: 5px 2px 14px rgba(0, 0, 0, 0.15);\r\n  border-radius: 2px;\r\n  border:1px solid $fontColor;\r\n  cursor:pointer;\r\n\r\n  span{\r\n    font-weight: bold;\r\n    font-size: 16px;\r\n    line-height: 150%;\r\n    color: $fontColor;\r\n  }\r\n}\r\n\r\n.price-card__buttons{\r\n  @include flex(center, flex-start);\r\n}\r\n\r\n.swiper-pagination-bullet{\r\n  background:red;\r\n}\r\n\r\n.portfolio-swiper-pagination .swiper-pagination-bullet-active{\r\n  background:$fontColor!important;\r\n}\r\n\r\n.portfolio-swiper-arrow-left, .portfolio-swiper-arrow-right{\r\n  background:#fff;\r\n  width:60px;\r\n  height:60px;\r\n  position:absolute;\r\n  top:50%;\r\n  transform:translateY(-50%);\r\n  left:0;\r\n  @include flex(center, center);\r\n  cursor:pointer;\r\n}\r\n\r\n.portfolio-swiper-arrow-right{\r\n  left:calc(100% - 60px);\r\n}\r\n","scss\\mixins.scss":"@mixin flex($a: center, $b: center, $c: row, $d: no-wrap){\r\n  display:flex;\r\n  align-items:$a;\r\n  justify-content:$b;\r\n  flex-direction:$c;\r\n  flex-wrap: $d;\r\n}\r\n","scss\\variables.scss":"$backgroundColor: #F0F0F0;\r\n$fontColor: #30475E;\r\n$secondaryFontColor: #4C4D52;\r\n$red: #F05454;\r\n"},"lineCount":null}},"error":null,"hash":"ee95d77cb2df85f6a0f57020f59f9d30","cacheData":{"env":{}}}